#include <stdio.h>

int main() 
{
    float num1,num2,result;
    char op;
    printf("welcome to calculator\n");
    printf("(number 1)(+-*/)(number 2)\n");
    scanf("%.2f %.2f", &num1,&num2);

    
    switch (op)
{
    case '+': result=num1+num2;
    printf("addition of numbers %.2f");
    break;
    
    case '-': result=num1-num2;
    printf("subtraction of numbers %.2f");
    break;
    
    case '*': result=num1*num2;
    printf("multiplication of numbers %.2f");
    break;
    
    case '/':result=num1/num2;
    printf("division of numbers %.2f");
    break;
    
    default:
    printf("invalid input");
    break;
}
    return 0;
}
